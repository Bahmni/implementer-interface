    "react-file-download": "^0.3.3",
    "react-file-reader": "^1.0.2",
    "react-redux": "^4.4.5",
    "react-router": "^2.6.0",
    "redux": "^3.6.0",
    "whatwg-fetch": "^1.0.0",
    "js-beautify" : "^1.10.3"
  }
